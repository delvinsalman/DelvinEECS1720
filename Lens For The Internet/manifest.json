{
 "name": "Exetension Project!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Website Blocker And Background Color Changer!", 
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
  "scripts": ["background.js"]
},
  "content_scripts": [
  {
          
  "matches": ["<all_urls>"],
  "js": ["content.js"]
}
  ],
  "browser_action": {
  "default_popup":"popup.html"
}
,


  "icons": {
    "16": "icons/D.png",
    "48": "icons/D.png",
    "128": "icons/D.png"
  }
}
  